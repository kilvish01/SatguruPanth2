{
  "expo": {
    "name": "Satguru Panth",
    "slug": "satgurupanth",
    "platforms": [
      "ios",
      "android",
      "web"
    ],
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./src/assets/images/icon.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "splash": {
      "image": "./src/assets/images/icon.png",
      "resizeMode": "contain",
      "backgroundColor": "#E3E0CF"
    },
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.satgurupanth.satguruapp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./src/assets/images/icon.png",
        "backgroundColor": "#E3E0CF"
      },
      "package": "com.satgurupanth.satguruapp"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./src/assets/images/icon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "image": "./src/assets/images/icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#E3E0CF"
        }
      ],
      "expo-font",
      "expo-video",
      "@react-native-community/datetimepicker",
      "expo-web-browser"
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "96fe974c-0d33-4e6a-a9e4-6a6da88c1b8d"
      }
    },
    "owner": "kilvish"
  }
}
